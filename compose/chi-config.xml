
<clickhouse>

    <logger>
        <level>debug</level>
        <console>1</console>
    </logger>

    <proxy>
        <disable_tunneling_for_https_requests_over_http_proxy>1</disable_tunneling_for_https_requests_over_http_proxy>
        <https>
            <uri>http://nginx:8080</uri>
        </https>
        <http>
            <uri>http://nginx:8080</uri>
        </http>
    </proxy>
</clickhouse>